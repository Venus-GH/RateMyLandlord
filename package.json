{
  "name": "rate-my-landlord",
  "version": "2.0.0",
  "description": "Some boilerplate code to get you started - get shakin'!",
  "engines": {
    "node": "~12.11.1"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.17",
    "@react-google-maps/api": "^2.1.1",
    "@storybook/addon-actions": "^6.1.21",
    "axios": "^0.19.0",
    "chart.js": "^2.9.4",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^6.0.0",
    "emailjs": "^3.4.0",
    "emailjs-com": "^2.6.4",
    "express": "^4.16.4",
    "express-session": "^1.15.1",
    "firebase": "^8.3.1",
    "firebaseui": "^4.8.0",
    "google-maps-react": "^2.0.6",
    "history": "^4.9.0",
    "leaflet": "^1.7.1",
    "mapbox-gl": "^2.1.1",
    "materialize-css": "^1.0.0-rc.2",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "multiselect-react-dropdown": "^1.6.11",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^8.3.2",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "puppeteer": "^8.0.0",
    "query-string": "^6.14.1",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^16.8.6",
    "react-google-maps-loader": "^4.3.0",
    "react-google-places-suggest": "^3.11.0",
    "react-icons": "^4.2.0",
    "react-leaflet": "^3.1.0",
    "react-map-gl": "^6.1.10",
    "react-materialize": "^3.9.7",
    "react-minimal-pie-chart": "^8.2.0",
    "react-places-street-input": "^1.0.9",
    "react-ratings-declarative": "^3.4.1",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-star-rating": "^1.4.2",
    "react-tagcloud": "^2.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.3.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
